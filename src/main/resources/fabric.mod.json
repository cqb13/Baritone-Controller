{
  "schemaVersion": 1,
  "id": "baritone-controller",
  "version": "${version}",
  "name": "Baritone Controller",
  "description": "Provides a simple frontend for baritone commands.",
  "authors": ["cqb13"],
  "contact": {
    "sources": "https://github.com/cqb13/Baritone-Controller"
  },
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": ["dev.cqb13.BaritoneController.BaritoneController"]
  },
  "mixins": ["baritone-controller.mixins.json"],
  "custom": {
    "meteor-client:color": "78,100,135",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
